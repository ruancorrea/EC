        <label/>
        <text>fun inverte3(l, a)=
if a = []
then l
else inverte3(List.hd a::l, List.tl a);

fun mem(a, l,p1,p2,x)=
if l = []
then inverte3([], a)
else if x = p1
then mem(List.hd l::a, List.tl l, p1,p2,x+1)
else if x = p2
then mem(List.hd l::a, List.tl l, p1,p2,x+1)
else mem(a, List.tl l, p1,p2, x+1);


mem([],[1,3,2,1,4],1,3,0)

(*nao tinha entendido bem o anunciado *)</text>
