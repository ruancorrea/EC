        <label/>
        <text>fun inverte(l,a) =
if a = []
then l
else inverte(List.hd a::l, List.tl a);

fun tenis(a, x: (int*string) list, n, cop) =
if n = []
then inverte([],a)

else if x = []
then tenis(a, cop, n, x)

else if (#2(List.hd x) = List.hd n)
then tenis(#1(List.hd x) :: a, List.tl x, List.tl n, List.hd x::cop)

else tenis(a, List.tl x, n, List.hd x::cop);

tenis([], [(1,&quot;x&quot;), (2,&quot;y&quot;), (3,&quot;z&quot;), (4,&quot;a&quot;), (5,&quot;b&quot;)], [&quot;z&quot;,&quot;a&quot;, &quot;x&quot;],[])</text>
