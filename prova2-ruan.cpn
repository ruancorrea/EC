        <label/>
        <text>fun inverte4(q,l)=
if l = []
then q
else inverte4(List.hd l::q, List.tl l);

fun add(e,l,x) =
if l = []
then inverte4(l,e)
else if x = 1 
then add(List.hd l::e, List.tl l,x)
else inverte4(l,e);

fun mem(a, e, l,x) = 
if x &gt; 0
then add(e,l,x)

else if l = []
then mem(a,a::e, l,2)

else if (a &gt; List.hd l)
then mem(a, List.hd l::e, List.tl l,x)

else if (a &lt;= List.hd l)
then mem(a,a::e, l,1)

else mem(a, e,[],0);

mem(6, [], [1,2,4,5],0);

(*nao deu tempo de arrumar para o maior valor*)</text>
